var num1 = 7;
var num2 = 6;
// console.log(num1 + num2);

var soma
var resultado

function calcularSoma(num1, num2) {
  soma = num1 + num2;
  return soma;
}

resultado = calcularSoma(num1, num2);
console.log('a soma de', num1, 'e', num2, 'é', resultado);

num1 = 50
num2 = 43

resultado = calcularSoma(num1, num2);
console.log('a soma de', num1, 'e', num2, 'é', resultado);
